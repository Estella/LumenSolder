{
  "name": "solder/solderapi",
  "type": "application",
  "description": "Technic Solder API using Lumen Framework",
  "keywords": ["technic", "solder", "api"],
  "homepage": "https://github.com/TechnicPack/LumenSolder",
  "license": "MIT",
  "authors": [{
               "name": "Dylan Page",
               "email": "genpage@pagefortress.com",
               "homepage": "http://www.pagefortress.com",
               "role": "Developer"
                }],
    "require": {
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "illuminate/redis": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
